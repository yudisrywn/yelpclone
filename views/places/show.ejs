<% layout('layouts/app') %>
<h1><%= place.title %></h1>
<h2><%= place.location %></h2>
<form
  action="/places/<%= place._id %>?_method=delete"
  method="post"
  style="display: inline !important">
  <button type="submit">Delete</button>
</form>
<a href="/places/<%= place._id %>/edit">Edit</a>
<a href="/places">All Places</a>
